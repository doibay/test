    <template title="xentag_sidebar_cloud_mod" version_id="10" version_string="1.0"><![CDATA[<xen:if hascontent="true">
	<xen:require css="tinhte_xentag.css" />

	<div class="section Tinhte_XenTag_SidebarCloud Tinhte_XenTag_TagCloud">
		<div class="secondaryContent">
			<h3><a href="{xen:link {xen:helper Tinhte_XenTag_getOption, routePrefix}}">{xen:phrase tinhte_xentag_tag_cloud}</a></h3>
			<xen:contentcheck>
				<div id="flashcontent">This will be shown to users with no Flash or Javascript.</div>
				<script type="text/javascript" src="js/XenTagMod/swfobject.js"></script>
				<script type="text/javascript">
					var so = new SWFObject("js/XenTagMod/tagcloud.swf", "tagcloud", "{xen:property xentag_mod_tagcloud_sidebar_width}", "{xen:property xentag_mod_tagcloud_sidebar_height}", "7", "#ffffff");
					var tcolor = "{xen:property xentag_mod_tagcloud_sidebar_tcolor}";
					tcolor = tcolor.replace("#","0x");
					so.addVariable("tcolor", tcolor);
					var tcolor2 = "{xen:property xentag_mod_tagcloud_sidebar_tcolor2}";
					tcolor2 = tcolor2.replace("#","0x");
					so.addVariable("tcolor2", tcolor2);
					var hicolor = "{xen:property xentag_mod_tagcloud_sidebar_hicolor}";
					hicolor = hicolor.replace("#","0x");
					so.addVariable("hicolor", hicolor);
					so.addVariable("mode", "tags");
					so.addVariable("distr", "true");
					so.addVariable("tspeed", "{xen:property xentag_mod_tagcloud_sidebar_tspeed}");
					so.addVariable("tagcloud", "<tags><xen:hook name="xentag_tag_cloud_item_mod" params="{xen:array 'max={$widget.options.limit}'}"><a href='{TAG_LINK}' style='font-size: {TAG_LEVEL}'>{TAG_TEXT}</a></xen:hook></tags>");
					so.write("flashcontent");
				</script>
			</xen:contentcheck>	
		</div>
	</div>
</xen:if>]]></template>
    <template title="xentag_tag_list_mod" version_id="10" version_string="1.0"><![CDATA[<xen:require css="tinhte_xentag.css" />
<xen:require js="js/Tinhte/XenTag/frontend.js" />

<xen:title>{xen:phrase tinhte_xentag_tags}</xen:title>
<xen:description>
	{xen:phrase tinhte_xentag_these_are_x_most_used_tags, 'count={xen:count $tags}'}
</xen:description>

<xen:container var="$head.canonical">
	<link rel="canonical" href="{xen:link 'canonical:{xen:helper Tinhte_XenTag_getOption, routePrefix}'}" /></xen:container>

<xen:container var="$canSearch">0</xen:container>

<fieldset>
	<ul class="Tinhte_XenTag_TagCloud sectionMain">
		<div id="flashcontentbig">This will be shown to users with no Flash or Javascript.</div>
	<script type="text/javascript" src="js/XenTagMod/swfobject.js"></script>
            <script type="text/javascript">
		var so = new SWFObject("js/XenTagMod/tagcloud.swf", "tagcloud", "{xen:property xentag_mod_tagcloud_main_width}", "{xen:property xentag_mod_tagcloud_main_height}", "7", "#ffffff");
		so.addParam("wmode", "transparent");
var tcolor = "{xen:property xentag_mod_tagcloud_main_tcolor}";
tcolor = tcolor.replace("#","0x");
		so.addVariable("tcolor", tcolor);
var tcolor2 = "{xen:property xentag_mod_tagcloud_main_tcolor2}";
tcolor2 = tcolor2.replace("#","0x");
		so.addVariable("tcolor2", tcolor2);
var hicolor = "{xen:property xentag_mod_tagcloud_main_hicolor}";
hicolor = hicolor.replace("#","0x");
		so.addVariable("hicolor", hicolor);
		so.addVariable("mode", "tags");
		so.addVariable("distr", "true");
		so.addVariable("tspeed", "{xen:property xentag_mod_tagcloud_main_tspeed}");
		so.addVariable("tagcloud", "<tags><xen:foreach loop="$tags" value="$tag"><a href='{xen:link {xen:helper Tinhte_XenTag_getOption, routePrefix}, $tag}' style='font-size: <xen:if is="{$tag.cloudLevel}==1">{xen:property xentag_mod_main_size_1}<xen:elseif is="{$tag.cloudLevel}==2" />{xen:property xentag_mod_main_size_2}<xen:elseif is="{$tag.cloudLevel}==3" />{xen:property xentag_mod_main_size_3}<xen:elseif is="{$tag.cloudLevel}==4" />{xen:property xentag_mod_main_size_4}<xen:elseif is="{$tag.cloudLevel}==5" />{xen:property xentag_mod_main_size_5}</xen:if>'>{$tag.tag_text}</a></xen:foreach></tags>");
		so.write("flashcontentbig");
	</script>
</fieldset>

<form action="{xen:link '{xen:helper Tinhte_XenTag_getOption, routePrefix}/search'}" method="post" class="xenForm">
	<h3 class="textHeading">{xen:phrase tinhte_xentag_search_by_tag}</h3>

	<dl class="ctrlUnit">
		<dt><label for="ctrl_tinhte_xentag_tags">{xen:phrase tinhte_xentag_tags}:</label></dt>
		<dd>
			<input type="text"
				name="tinhte_xentag_tags_text"
				value=""
				id="ctrl_tinhte_xentag_tags"
				class="textCtrl AutoComplete AcSingle"
				data-acUrl="{xen:link '{xen:helper Tinhte_XenTag_getOption, routePrefix}/find'}"
				data-autoSubmit="yes"
				/>
			<input type="hidden" name="tinhte_xentag_included" value="1" />
		</dd>
	</dl>

	<dl class="ctrlUnit submitUnit">
		<dt></dt>
		<dd><input type="submit" value="{xen:phrase search}" class="button primary" /></dd>
	</dl>

	<input type="hidden" name="_xfToken" value="{$visitor.csrf_token_page}" />
</form>

<xen:include template="tinhte_xentag_copyright" />]]></template>
